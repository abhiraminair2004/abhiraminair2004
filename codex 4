# include <iostream>
#include<cmath>
using namespace std;
int main() {
    char ch;
    float num1, num2;
    cout<< "Enter operator: +, -, *, /,|(for absolute):";
    cin>>ch;
    cout<<"Enter number 1:";
    cin>>num1;
    cout<<"Enter number 2:";
    cin>>num2;
    switch(ch){
        case '+':
        cout<<"Sum:"<<num1+num2;
        break;
        case '-':
        cout<<"Difference:"<<num1-num2;
        break;
        case '*':
        cout<<"Product:"<<num1*num2;
        break;
        case '/':
        cout<<"Division:"<<num1/num2;
        break;
        case '|':
        cout<<"Absolute value 1:"<<abs(num1)<<endl;
        cout<<"Absolute value 2:"<<abs(num2)<<endl;
        break;
        default:
        cout<<"ERROR";
        break;
        }

}
